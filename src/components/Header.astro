    .nav-link {
        text-align: center;
        flex: 0 1 25%;
        margin: auto;
        transition-duration: 250ms;
    }

    .nav-link:hover {
        transform: scale(1.2);
    }
    
    .link > a {
        text-decoration: none;
        color: white;
        padding: 1rem;
        margin: 0 2rem;
        transition-duration: 250ms;
    }

    .icon > svg {
        height: 25px;
        width: 25px;
    }
</style>

